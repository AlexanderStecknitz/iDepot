<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="002-dev-load-investment-data" author="Alexander Stecknitz" context="dev">
        <loadData tableName="investment" file="db/changelog/dev/data/investments.csv">
            <column name="investment_id" type="serial"/>
            <column name="stock_isin" type="text"/>
            <column name="depot_id" type="serial"/>
            <column name="amount" type="numeric"/>
            <column name="buy_price" type="numeric"/>
        </loadData>
    </changeSet>

</databaseChangeLog>